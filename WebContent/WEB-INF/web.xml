<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Proyecto</display-name>
  <!--JSP config-->
  <jsp-config>
    <taglib>
      <taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
      <taglib-location>/WEB-INF/c.tld</taglib-location>
    </taglib>
    <taglib>
      <taglib-uri>http://java.sun.com/jsp/jstl/core-rt</taglib-uri>
      <taglib-location>/WEB-INF/c-rt.tld</taglib-location>
    </taglib>
    <taglib>
      <taglib-uri>http://aitools.org/programd/4.6/taglib/aiml</taglib-uri>
      <taglib-location>/WEB-INF/aiml.tld</taglib-location>
    </taglib>
    <taglib>
      <taglib-uri>http://java.sun.com/jsp/jstl/functions</taglib-uri>
      <taglib-location>/WEB-INF/fn.tld</taglib-location>
    </taglib>
  </jsp-config>   
  
   <servlet>
    <servlet-name>AgentServlet</servlet-name>
    <servlet-class>es.upm.dit.gsi.AgentServlet</servlet-class>
   	 <init-param>
        <param-name>path</param-name>
        <param-value>/Users/lorenasanchomayoral/Documents/workspace/Proyecto/WebContent/WEB-INF/models/Thermostat.xml</param-value>
     </init-param>
    
  </servlet>
  <servlet-mapping>
    <servlet-name>AgentServlet</servlet-name>
    <url-pattern>/AgentServlet</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
    <welcome-file>Agent.jsp</welcome-file>
  </welcome-file-list>
 
</web-app>